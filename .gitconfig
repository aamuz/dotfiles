[credential]
	helper = !/opt/homebrew/Cellar/openjdk/15.0.2/libexec/openjdk.jdk/Contents/Home/bin/java -Ddebug=false -Djava.net.useSystemProxies=true -jar /opt/homebrew/Cellar/git-credential-manager/2.0.4_2/libexec/git-credential-manager-2.0.4.jar
	helper =
	helper = /usr/local/share/gcm-core/git-credential-manager-core
[credential "https://dev.azure.com"]
	useHttpPath = true
[user]
	email = aamuz@users.noreply.github.com
	name = Amuz Tamrakar
[core]
	editor = code --wait
	autocrlf = input
[alias]
	s = status -s
	pr = pull --rebase
	st = status
	br = branch
	cb = branch --show-current
	lo = log --oneline
	hist = log --pretty=format:\"%h %ad | %s%d [%n]\" --graph --date=short
	type = cat-file -t
	dum = cat-file -p
	fp = fetch --prune
	ba = branch -a
[init]
	defaultBranch = prod
